{
  "name": "ben-gibson/bryn-fallback-calculator",
  "description": "An exchange rate calculator that defers calculation to its registered calculators in priority order until an exchange rate is successfully returned.",
  "keywords": [
    "currency",
    "conversion",
    "exchange",
    "rate",
    "calculator",
    "fallback"
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "homepage": "https://github.com/ben-gibson/bryn-fallback-calculator",
  "authors": [
    {
      "name": "Ben Gibson",
      "email": "ben.gibson.2011@gmail.com",
      "homepage": "https://ben-gibson.co.uk"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/ben-gibson/bryn"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ben-gibson/bryn-european-central-bank-calculator"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ben-gibson/bryn-yahoo-calculator"
    }
  ],
  "require": {
    "php": "~7.0",
    "ben-gibson/bryn": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit" : "~6.0",
    "squizlabs/php_codesniffer": "~2.3",
    "php-http/guzzle6-adapter": "^1.1",
    "ben-gibson/bryn-european-central-bank-calculator": "dev-master",
    "ben-gibson/bryn-yahoo-calculator": "dev-master"

  },
  "autoload": {
    "psr-4": {
      "Gibbo\\Bryn\\Calculator\\Fallback\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gibbo\\Bryn\\Calculator\\Fallback\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "format": "phpcbf --standard=psr2 src/"
  }
}
